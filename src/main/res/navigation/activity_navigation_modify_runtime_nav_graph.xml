<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_navigation_modify_runtime_nav_graph"
    app:startDestination="@+id/info">

    <navigation
        android:id="@+id/info"
        app:startDestination="@+id/modify_nav_graph_info">

        <fragment
            android:id="@+id/modify_nav_graph_info"
            android:name="com.example.navigation.interaction.modifygraph.NavigationModifyNavGraphInfoFragment"
            android:label="Info"
            tools:layout="@layout/fragment_navigation_modify_nav_graph_info" />

    </navigation>

    <navigation
        android:id="@+id/settings"
        app:startDestination="@+id/modify_nav_graph_settings">

        <fragment
            android:id="@+id/modify_nav_graph_settings"
            android:name="com.example.navigation.interaction.modifygraph.NavigationModifyNavGraphSettingsFragment"
            android:label="Settings"
            tools:layout="@layout/fragment_navigation_modify_nav_graph_settings" />
    </navigation>

</navigation>
